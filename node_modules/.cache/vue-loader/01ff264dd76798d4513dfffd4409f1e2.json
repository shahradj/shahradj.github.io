eReader = new FileReader();\n        fileReader.readAsArrayBuffer(file);\n        fileReader.addEventListener('load', () => {\n          this.$store.dispatch('addTrack', {\n            name,\n            arrayBuffer: fileReader.result\n          });\n        });\n      });\n      this.files = [];\n    }\n  }\n};\n</script>\n"]}]}